[core]
	editor = vim
[user]
	name = wooner49
	email = wooner49@gmail.com
[alias]
  b = branch
  br = branch -r
  ba = branch -a
  bv = branch -v
	co = checkout
  g = log --graph --decorate --pretty=oneline --abbrev-commit --all --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)'
  g2 = log --graph --date-order --date=short --pretty=format:'%C(auto)%h%d %C(reset)%s %C(bold blue)%ce %C(reset)%C(green)%cr (%cd)'
  st = status
